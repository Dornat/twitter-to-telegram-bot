{
    "name": "dornat/twitter-to-telegram-bot",
    "description": "V2 of Twitter to Telegram bot channel manager.",
    "type": "project",
    "keywords": ["twitter", "telegram", "bot", "console", "cli", "php", "manager"],
    "homepage": "https://github.com/dornat/twitter-to-telegram-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Dornat",
            "email": "mrdornat@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dornat/twitter-to-telegram-bot/issues",
        "source": "https://github.com/dornat/twitter-to-telegram-bot"
    },
    "require": {
        "php": "^8.1",
        "abraham/twitteroauth": "^4.0",
        "laravel-zero/framework": "^8.8",
        "spatie/twitter-streaming-api": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ttt"]
}
